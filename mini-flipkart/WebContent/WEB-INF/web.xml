<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>mini-flipkart</display-name>
  <servlet>
    <servlet-name>signup</servlet-name>
    <servlet-class>signup</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>signin</servlet-name>
    <servlet-class>signin</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>sellerStore</servlet-name>
    <servlet-class>sellerStore</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>store</servlet-name>
    <servlet-class>store</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>store</servlet-name>
    <url-pattern>/store</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>signup</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>signin</servlet-name>
    <url-pattern>/signin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>sellerStore</servlet-name>
    <url-pattern>/sellerStore</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>signup-page.html</welcome-file>
  </welcome-file-list>
</web-app>